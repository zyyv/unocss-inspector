{
  "name": "@unoc-inspect/monorepo",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.16.1",
  "author": "Chris",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm lint --fix",
    "dev": "pnpm -r --filter=./packages/inspector run play",
    "build": "pnpm -r --filter=./packages/* run build",
    "play": "pnpm --filter ./playground/use-plugin dev",
    "play:plugin": "pnpm --filter ./playground/use-plugin build",
    "play:component": "pnpm --filter ./playground/component build",
    "deploy": "pnpm build && pnpm play:component",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:lint",
    "@iconify-json/catppuccin": "catalog:icons",
    "@iconify-json/hugeicons": "catalog:icons",
    "@iconify/utils": "catalog:icons",
    "@types/node": "catalog:utils",
    "@unocss/core": "catalog:unocss",
    "@unocss/preset-web-fonts": "catalog:unocss",
    "@unocss/preset-wind4": "catalog:unocss",
    "@vitejs/plugin-vue": "catalog:vue",
    "debug": "catalog:utils",
    "eslint": "catalog:lint",
    "esno": "catalog:utils",
    "tinyglobby": "catalog:utils",
    "tsdown": "catalog:build",
    "tsx": "catalog:build",
    "typescript": "catalog:build",
    "unocss": "catalog:unocss",
    "vite": "catalog:build",
    "vite-plugin-dts": "catalog:build",
    "vue": "catalog:vue",
    "vue-tsc": "catalog:vue"
  }
}
