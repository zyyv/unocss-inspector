{
  "name": "unocss-inspector",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@10.15.0",
  "description": "Interactive dynamic review elements for UnoCSS",
  "author": "Chris",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "inspector",
    "unocss",
    "element-inspector",
    "dom-inspector",
    "component",
    "typescript"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "play": "vite",
    "play:build": "vite build",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@vueuse/core": "^13.8.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@iconify-json/catppuccin": "^1.2.11",
    "@iconify-json/hugeicons": "^1.2.5",
    "@types/node": "^22.18.0",
    "@unocss/core": "^66.5.0",
    "@unocss/preset-web-fonts": "^66.4.2",
    "@unocss/preset-wind4": "^66.5.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^9.34.0",
    "tsdown": "^0.14.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "unocss": "^66.5.0",
    "vite": "^7.1.3",
    "vue": "^3.5.20",
    "vue-tsc": "^3.0.6"
  }
}
