{
  "name": "@uno-inspect/inspector",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.16.1",
  "description": "Interactive dynamic review elements for UnoCSS",
  "author": "Chris",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "inspector",
    "unocss",
    "element-inspector",
    "dom-inspector",
    "component",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    },
    "./css": "./dist/index.css"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "postbuild": "esno scripts/copy-to-unplugin.ts",
    "dev": "vite build --watch"
  },
  "peerDependencies": {
    "vue": "catalog:vue"
  },
  "dependencies": {
    "@vueuse/core": "catalog:vue"
  },
  "devDependencies": {
    "vite-dev-rpc": "catalog:plugins"
  }
}
